<%= simple_form_for @member do |f| %>
	<%= render 'shared/error_messages', modelo: f.object %>
	<%= render :partial => 'parceiros/form', :locals => {:f => f, :parceiro => :parceiro }%>
	
	<fieldset>
		<legend>Dados do Membro</legend>
		<%= f.input :entrada, as: :date %>
	</fieldset>
	<br>
	<%= f.button :submit,'Salvar', :class => 'button' %>
<% end %>